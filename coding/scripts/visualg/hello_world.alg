algoritmo "HelloWorld"
// Algoritmos e Lógica de Programação

var name, lastname, fullname: caractere;

inicio
    escreval("Hello world!");
    escreva("Nome: ");
    leia(name);
    escreva("Sobrenome: ");
    leia(lastname);
    fullname <- name + " " + lastname;
    escreval("Bem-vindo " + fullname); 

fimalgoritmo